    SEPARATOR
    BUTTON      ID_FILE_PRINT
    SEPARATOR
    BUTTON      ID_APP_ABOUT
END


/////////////////////////////////////////////////////////////////////////////
//
// Menu
//

IDR_MAINFRAME MENU DISCARDABLE 
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "E&xit",                       ID_APP_EXIT
    END
    POPUP "&Edit"
    BEGIN
        MENUITEM "&Undo\tCtrl+Z",               ID_EDIT_UNDO
        MENUITEM SEPARATOR
        MENUITEM "Cu&t\tCtrl+X",                ID_EDIT_CUT
        MENUITEM "&Copy\tCtrl+C",               ID_EDIT_COPY
        MENUITEM "&Paste\tCtrl+V",              ID_EDIT_PASTE
        MENUITEM SEPARATOR
        MENUITEM "Select &All",                 ID_EDIT_SELECT_ALL
    END
    POPUP "&View"
    BEGIN
        MENUITEM "&Menu bar",                   ID_VIEW_MENUBAR
        POPUP "&Toolbars"
        BEGIN
            MENUITEM "Standard &Toolbar",           ID_VIEW_TOOLBAR
            MENUITEM "UI Theme S&witcher Toolbar",  ID_VIEW_UI_LOOK_BAR
        END
        POPUP "&Resizable bars"
        BEGIN
            MENUITEM "&Resizable bar 0",            ID_VIEW_RESIZABLE_BAR_0
            MENUITEM "&Resizable bar 1",            ID_VIEW_RESIZABLE_BAR_1
            MENUITEM "&Resizable bar 2",            ID_VIEW_RESIZABLE_BAR_2
            MENUITEM "&Resizable bar 3",            ID_VIEW_RESIZABLE_BAR_3
            MENUITEM "&Resizable bar 4",            ID_VIEW_RESIZABLE_BAR_4
        END
        MENUITEM "&Status Bar",                 ID_VIEW_STATUS_BAR
        MENUITEM SEPARATOR
        POPUP "&UI theme"
        BEGIN
            MENUITEM "<<< Prof-UIS UI Themes List >>>", 29999
        END
        MENUITEM SEPARATOR
        MENUITEM "&Display color controls",     ID_VIEW_COLOR_CONTROLS
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&About SDI...",               ID_APP_ABOUT
    END
END


